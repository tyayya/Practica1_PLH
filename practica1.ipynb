   "source": [
    "def extract_ngrams(text, n):   # MIRAR SI ES POR OPTIMITZAR AMB SPACY\n",
    "    \"\"\"Extreu tots els n-grames de car√†cters d'un text i retorna un Counter.\"\"\"\n",
    "    ngrams = Counter()\n",
    "    for i in range(len(text) - n + 1):\n",
    "        ngram = text[i:i+n]\n",
    "        ngrams[ngram] += 1\n",
    "    return ngrams\n",
    "\n",
    "def filter_ngrams(ngram_counts, min_count=5):\n",
    "    \"\"\"Elimina n-grames amb freq√º√®ncia menor a min_count.\"\"\"\n",
    "    return Counter({ng: c for ng, c in ngram_counts.items() if c >= min_count})\n"
   ]
